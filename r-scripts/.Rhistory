library(reshape)
#Reads in files
pra     <-read.csv(paste("Y:/Daily Import Files/2016/2016 aug/08222016/PRA-Table.csv", sep=""), stringsAsFactors=FALSE)
pra2<-subset(pra,Subscriber.ID!="" )
pra3<-subset(pra2,PRA.Creation.Date!="" )
pra4<-subset(pra3,!grepl("NIC", pra3$Subscriber.ID))
